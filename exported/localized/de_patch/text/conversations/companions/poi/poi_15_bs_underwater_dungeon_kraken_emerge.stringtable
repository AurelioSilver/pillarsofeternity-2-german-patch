<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\companions\poi\poi_15_bs_underwater_dungeon_kraken_emerge</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>5</EntryCount>
  <Entries>
    <Entry>
      <ID>3</ID>
      <DefaultText>„Mutter! Vergib mir dafür, dass ich diese Schönheit erschlage.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>„Du musst mir verzeihen, wenn ich das sage – aber das ist das letzte Mal, dass ich dir unter Wasser folge!“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>„Madiccho! Ich dachte, wir hätten dich bereits umgebracht!“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>„Pass auf, wohin du trittst! Mit Tentakeln ist nicht zu spaßen, egal was die Leute behaupten!“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>8</ID>
      <DefaultText>„Je größer sie sind – desto härter der Sturz.“</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>
